{
    "name": "boo/lua-parser",
    "description": "A Lua parse aiming to be compatible with anything found within World of Warcraft's WTF directory.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
    ],
    "support": {
       "issues": "https://github.com/jyggen/lua-parser/issues",
       "source": "https://github.com/jyggen/lua-parser"
    },
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "doctrine/lexer": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v2.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "pds/skeleton": "^1.0.0",
        "phpmd/phpmd": "^2.6.0",
        "phpro/grumphp": "^0.11.6",
        "phpstan/phpstan": "^0.7.0",
        "phpunit/phpunit": "^6.2.2",
        "povils/phpmnd": "^1.1.1",
        "sebastian/diff": "^1.4.3",
        "sebastian/phpcpd": "^3.0.0",
        "symfony/console": "^3.3.2",
        "symfony/var-dumper": "^3.3.2",
        "symfony/finder": "^3.3.2"
    },
    "autoload": {
        "psr-4": {
            "Boo\\LuaParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boo\\LuaParser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
