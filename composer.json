{
    "name": "boo/lua-parser",
    "description": "A Lua parse aiming to be compatible with anything found within World of Warcraft's WTF directory.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
    ],
    "support": {
       "issues": "https://github.com/jyggen/lua-parser/issues",
       "source": "https://github.com/jyggen/lua-parser"
    },
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "doctrine/lexer": "^1.0"
    },
    "require-dev": {
        "boo/code-standard": "^0.1.0",
        "pds/skeleton": "^1.0.0",
        "phpunit/phpunit": "^6.2.4",
        "sebastian/diff": "^1.4.3",
        "symfony/console": "^3.3.6",
        "symfony/finder": "^3.3.6",
        "symfony/var-dumper": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Boo\\LuaParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boo\\LuaParser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
